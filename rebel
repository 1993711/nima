package Sorted;

import java.util.Arrays;
import java.util.List;
import java.util.stream.Collectors;

public class DemoArrays{
	
	
	public static void main(String[] args) {
		
		Integer [] num = {1,9,8,5,3,62,0, 100};
	
		System.out.printf("Same as given : %s%n", Arrays.asList(num));
		
		System.out.printf(" Assending or sorted order: %s%n ", Arrays.stream(num).sorted().collect(Collectors.toList()));
